#include<stdio.h>
#include<conio.h>
int main()
{
    int n,i,sum=0,sq;
    printf("Enter the limit\n");
    scanf("%d",&n);
    printf("The sum of squares of first %d odd numbers are\n",n);
    for(i=1;i<=n*2;i++)\
    {
        if(i%2==1)
        {
            sq=i*i;
            sum=sum+sq;
        }
    }
    printf("sum=%d",sum);
    getch();
    return 0;
}
